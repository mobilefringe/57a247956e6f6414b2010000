<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
        show_cat_stores();
    })
    
    function renderPage(){
        var stores = [];
        var s1 = getStoresListByCategoryID(4342)
        var s2 = getStoresListByCategoryID(4354)
        var s3 = getStoresListByCategoryID(4344)
         var s4 = getStoresListByCategoryID(4336)
        var s5 = getStoresListByCategoryID(4339)
        var s6 = getStoresListByCategoryID(4332)
         var s7 = getStoresListByCategoryID(4353)
        var s8 = getStoresListByCategoryID(4355)
        var s9 = getStoresListByCategoryID(4347)
         var s10 = getStoresListByCategoryID(4331)
         var s11 = getStoresListByCategoryID(4349)
        var s12 = getStoresListByCategoryID(4333)
        var s13 = getStoresListByCategoryID(4334)
       
        stores = stores.concat(s1)
        stores = stores.concat(s2)
        stores = stores.concat(s3)
        stores = stores.concat(s4)
        stores = stores.concat(s5)
        stores = stores.concat(s6)
        stores = stores.concat(s7)
        stores = stores.concat(s8)
        stores = stores.concat(s9)
        stores = stores.concat(s10)
        stores = stores.concat(s11)
        stores = stores.concat(s12)
         stores = stores.concat(s13)
        
        var newarr = [];
        var unique = {};
        $.each(stores, function(i,v){
            if (!unique[v.slug]) {
                newarr.push(v);
                unique[v.slug] = v;
            }
        })
        newarr.sortBy(function(o){ return o.name })
        renderStoreList('#store_list_container','#store_list_template', newarr, "stores", "A", "M");
        renderStoreList('#store_list_container2','#store_list_template2', newarr, "stores","M", "Z" );
        // var categories = getStoreCategories();
        // renderGeneral('#category_container, #category_container2', '#category_template', categories);
        show_content();
        show_cat_stores();
    }
</script>